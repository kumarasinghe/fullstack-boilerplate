{
    "name": "my-qwik-empty-starter",
    "description": "App with Routing built-in ready to create your app",
    "engines": {
        "node": ">=18.11.0"
    },
    "private": true,
    "scripts": {
        "build": "qwik build",
        "build-storybook": "storybook build",
        "build.client": "vite build",
        "build.preview": "vite build --ssr src/entry.preview.tsx",
        "build.types": "tsc --incremental --noEmit",
        "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
        "dev": "vite --mode ssr",
        "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
        "format": "pretty-quick",
        "lint": "eslint \"src/**/*.ts*\"",
        "prepare": "husky install",
        "preview": "qwik build preview && vite preview --open",
        "qwik": "qwik",
        "start": "vite --open --mode ssr",
        "storybook": "storybook dev -p 6006",
        "test.unit": "vitest components",
        "test.unit.ui": "vitest --ui components"
    },
    "devDependencies": {
        "@builder.io/qwik": "^1.1.4",
        "@builder.io/qwik-city": "^1.1.4",
        "@storybook/addon-essentials": "7.0.11",
        "@storybook/addon-links": "7.0.11",
        "@storybook/blocks": "7.0.11",
        "@storybook/builder-vite": "7.0.11",
        "@storybook/html": "7.0.11",
        "@storybook/html-vite": "7.0.11",
        "@types/eslint": "8.37.0",
        "@types/node": "^20.1.4",
        "@typescript-eslint/eslint-plugin": "5.59.5",
        "@typescript-eslint/parser": "5.59.5",
        "@vitest/ui": "^0.31.0",
        "autoprefixer": "^10.4.14",
        "eslint": "8.40.0",
        "eslint-plugin-qwik": "^1.1.4",
        "husky": "^8.0.3",
        "postcss": "^8.4.23",
        "prettier": "2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "pretty-quick": "^3.1.3",
        "sass": "^1.62.1",
        "storybook": "7.0.11",
        "storybook-framework-qwik": "0.2.0",
        "tailwindcss": "^3.3.1",
        "typescript": "5.0.4",
        "undici": "5.22.1",
        "vite": "4.3.5",
        "vite-tsconfig-paths": "4.2.0",
        "vitest": "^0.31.0"
    },
    "dependencies": {
        "@fontsource-variable/inter": "^5.0.1",
        "mongoose": "^7.2.2",
        "normalize.css": "^8.0.1",
        "zod": "^3.21.4"
    }
}
